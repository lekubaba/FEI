doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		title 查额系统
		link(rel="shortcut icon" href="/images/logo.png")
		link(rel="bookmark" href="/images/logo.png")
		link(rel="icon" href="/images/logo.png" type="image/png")
		meta(name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0")
		meta(http-equiv="X-UA-Compatible" content="ie=edge")
		link(rel="apple-touch-icon" href="/images/logo.png")
		meta(name="apple-mobile-web-app-title" content="查额系统")
		meta(name="mobile-web-app-capable" content="yes")
		meta(name="apple-mobile-web-app-status-bar-style" content="white")
		link(rel="stylesheet" href="/styles/zonghe.css")
		script(src="/js/remind.js")
	body
		div.top_t
			img.chaedu_welcome(src="/images/fd_moneys.png")
			div.chaedu_profile
				div.chaedu_profile_1
					div.chaedu_profile_2 归属者
					div.chaedu_profile_2 归属者手机号
				div.chaedu_profile_a
					div.chaedu_aa 客户姓名
					div.chaedu_aa 申请时间
					div.chaedu_aa 生效时间
				div.chaedu_profile_a
					div.chaedu_aa 手机号
					div.chaedu_aa 下款额度
					div.chaedu_aa 预计佣金
			div.chaedu_content
				each item in rets
					div.chaedu_profile(style="margin-top:5px;")
						div.chaedu_profile_1
							div.chaedu_profile_3=item.ownername
							div.chaedu_profile_3=item.ownerNumber
						div.chaedu_profile_a
							div.chaedu_aaa=item.username
							div.chaedu_aaa=item.shenqingTime
							div.chaedu_aaa=item.shengxiaoTime
						div.chaedu_profile_a
							div.chaedu_aaa=item.number
							div.chaedu_aaa=item.xiakuanEdu
							div.chaedu_aaa(style="color:red;")=item.money